	<groupId>com.patients</groupId>
	<artifactId>gdpr</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>Gdpr</name>
	<description>Patients</description>

	<parent>
